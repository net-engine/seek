<Job Reference="reference_value" TemplateID="template_id_value" ScreenID="screen_id_value">
  <Title>title_value</Title>
  <SearchTitle>search_title_value</SearchTitle>
  <Description>description_value</Description>
  <AdDetails><![CDATA[ad_details_value]]></AdDetails>
  <ApplicationEmail>application_email_value</ApplicationEmail>
  <ApplicationURL>application_url_value</ApplicationURL>
  <ResidentsOnly>Yes</ResidentsOnly>
  <Items>
    <Item Name="Jobtitle">item_job_title_value</Item>
    <Item Name="Bullet1">item_bullet1_value</Item>
    <Item Name="Bullet2">item_bullet2_value</Item>
    <Item Name="Bullet3">item_bullet3_value</Item>
    <Item Name="ContactName">item_contact_name_value</Item>
    <Item Name="PhoneNo">item_phone_number_value</Item>
    <Item Name="FaxNo">item_fax_number_value</Item>
    <Item Name="RefNumber">item_reference_number_value</Item>
  </Items>
  <Listing MarketSegments="listing_market_segments_value">
    <Classification Name="Location">listing_location_value</Classification>
    <Classification Name="Area">listing_area_value</Classification>
    <Classification Name="WorkType">listing_work_type_value</Classification>
    <Classification Name="Classification">listing_classification_value</Classification>
    <Classification Name="SubClassification">listing_sub_classification_value</Classification>
  </Listing>
  <Salary Type="salary_type_value" Min="salary_min_value" Max="salary_max_value" AdditionalText="salary_additional_text_value"/>
  <StandOut IsStandOut="Yes" LogoID="stand_out_logo_id_value" Bullet1="stand_out_bullet_1_value" Bullet2="stand_out_bullet_2_value" Bullet3="stand_out_bullet_3_value"/>
</Job>
